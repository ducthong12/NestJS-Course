{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "assets": [
      {
        "include": "proto/**/*.proto",
        "watchAssets": true
      },
      {
        "include": "prisma/**/*.prisma",
        "watchAssets": true
      },
      {
        "include": "generated/**/*",
        "watchAssets": false
      }
    ]
  }
}
